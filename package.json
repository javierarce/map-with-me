{
  "name": "MapWithMe",
  "version": "1.0.0",
  "description": "Create collaborative maps with your friends (and enemies)",
  "main": "server.js",
  "scripts": {
    "start": "npm run-script build && node server.js",
    "test": "mocha",
    "build": "cross-env browserify -g envify src/main.js | uglifyjs -m > public/build.js",
    "watch": "watchify -vd -p browserify-hmr -e src/main.js -o public/build.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "css-loader": "^3.2.0",
    "cssify": "^1.0.3",
    "dotenv": "^8.0.0",
    "emoji-regex": "^9.0.0",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "helmet": "^3.20.0",
    "leaflet": "^1.5.1",
    "node-cmd": "^3.0.0",
    "node-geocoder": "^3.23.0",
    "node-sass": "^4.12.0",
    "node-sass-middleware": "^0.11.0",
    "passport": "^0.4.0",
    "passport-twitter": "^1.0.4",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.2",
    "rss": "^1.2.2",
    "sanitize-html": "^1.20.1",
    "sass-loader": "^8.0.2",
    "sequelize": "^5.21.12",
    "sequelize-cli": "^5.4.0",
    "sqlite3": "^4.0.8",
    "style-loader": "^1.0.0",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^7.2.0",
    "browserify": "^13.0.1",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^7.0.2",
    "envify": "^4.1.0",
    "mocha": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "uglify-js": "^3.9.4",
    "vue": "^2.0.1",
    "vue-router": "^3.0.6",
    "vue-template-compiler": "^2.6.10",
    "vueify": "^9.1.0",
    "watchify": "^3.4.0"
  },
  "engines": {
    "node": "10.14.x"
  },
  "repository": {
    "url": "https://github.com/javierarce/map-with-me"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "license": "MIT",
  "keywords": [
    "node",
    "express",
    "mapping",
    "places",
    "collaborative"
  ]
}
